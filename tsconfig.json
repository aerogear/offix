{
  "compilerOptions": {
    "sourceMap": false,
    "strict": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "target": "es5",
    "lib": [
      "es6",
      "es2015",
      "esnext",
      "dom"
    ],
    "module": "CommonJS",
    "jsx": "react",
    "declaration": true,
    "types": ["jest", "node"]
  },
  "lib": [
     "esnext.asynciterable"
   ],
  "exclude": [
    "node_modules"
  ]
}
