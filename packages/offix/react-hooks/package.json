    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "apollo-cache": "1.3.5",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "cross-fetch": "3.0.6",
    "fake-indexeddb": "3.1.2",
    "graphql": "15.5.0",
    "jest": "26.6.3",
    "react-test-renderer": "17.0.1",
    "size-limit": "4.9.2",
    "ts-jest": "26.5.1",
