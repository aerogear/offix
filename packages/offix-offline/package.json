{
  "name": "offix-offline",
  "version": "0.15.0",
  "description": "Offix package that exposes network interfaces",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "del coverage src/**/*.js src/**/*.map test/**/*.js test/**/*.map dist types",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest --detectOpenHandles",
    "preintegration-test": "cd integration_test && npm install",
    "integration-test": "cd integration_test && npm test",
    "size": "size-limit",
    "size:why": "size-limit --why"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aerogear/offix.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.1.1",
    "apollo-link": "1.2.13",
    "graphql": "^14.6.0",
    "graphql-tag": "2.10.3",
    "jest": "25.1.0",
    "size-limit": "^4.1.1",
    "ts-jest": "25.2.0",
    "typescript": "3.7.5"
  }
}
