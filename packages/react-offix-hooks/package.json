  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "4.4.1",
    "@testing-library/react-hooks": "3.2.1",
    "@types/jest": "25.1.4",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.5",
    "apollo-cache": "1.3.5",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "cross-fetch": "3.0.4",
    "fake-indexeddb": "3.0.0",
    "graphql": "15.1.0",
    "jest": "25.5.4",
    "react-test-renderer": "16.13.1",
    "size-limit": "4.4.1",
    "ts-jest": "26.1.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "offix-client": "0.15.1",
    "react": "16.13.1"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0",
    "react": "16.13.1"
