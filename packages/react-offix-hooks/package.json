{
  "name": "react-offix-hooks",
  "version": "0.15.0",
  "description": "Use offix-client in react hooks",
  "keywords": [
    "offix",
    "react",
    "hooks"
  ],
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "del coverage src/**/*.js src/**/*.map test/**/*.js test/**/*.map dist types",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "preintegration-test": "cd integration_test && npm install",
    "integration-test": "cd integration_test && npm test",
    "size": "size-limit",
    "size:why": "size-limit --why"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aerogear/offix.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.1.1",
    "@testing-library/react-hooks": "3.2.1",
    "@types/jest": "25.1.4",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "apollo-cache": "1.3.4",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link": "1.2.13",
    "apollo-link-http": "1.5.16",
    "cross-fetch": "3.0.4",
    "fake-indexeddb": "3.0.0",
    "graphql": "14.6.0",
    "jest": "25.1.0",
    "react-test-renderer": "16.13.0",
    "size-limit": "^4.1.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "offix-client": "0.15.0"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0",
    "react": "16.13.0"
  }
}
