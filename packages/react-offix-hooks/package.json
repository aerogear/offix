    "react": "^16.8.6",
    "graphql": "14.4.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "offix-client": "0.7.1"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0",
    "react": "^16.8.6"
